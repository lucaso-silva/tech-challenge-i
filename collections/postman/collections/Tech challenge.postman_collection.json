          "name": "1. User client generator 4devs",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const responseData = pm.response.json();",
                  "",
                  "if (Array.isArray(responseData)) {",
                  "    ",
                  "    responseData.forEach((user) => {",
                  "",
                  "        const [localPart, dominio] = user.email.split(\"@\");",
                  "        ",
                  "        pm.environment.set(`client_name`, user.nome);",
                  "        pm.environment.set(`client_email`, user.email);",
                  "        pm.environment.set(`client_password`, user.senha);",
                  "        pm.environment.set(`client_login`, localPart)",
                  "        pm.environment.set(`client_street`, user.endereco);",
                  "        pm.environment.set(`client_number`, user.numero);",
                  "        pm.environment.set(`client_neighborhood`, user.bairro);",
                  "        pm.environment.set(`client_city`, user.cidade);",
                  "        pm.environment.set(`client_state`, user.estado);",
                  "        pm.environment.set(`client_zipCode`, user.cep);",
                  "    });",
                  "    ",
                  "} else {",
                  "    console.log(\"Resposta não é um array válido\");",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
