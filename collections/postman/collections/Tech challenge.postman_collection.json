          "name": "User owner generator 4devs",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const html = pm.response.text();",
                  "",
                  "function gerarSenha(tamanho = 8) {",
                  "    const chars = \"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789\";",
                  "    let senha = \"\";",
                  "",
                  "    for (let i = 0; i < tamanho; i++) {",
                  "        senha += chars.charAt(Math.floor(Math.random() * chars.length));",
                  "    }",
                  "",
                  "    return senha;",
                  "}",
                  "",
                  "function getValueById(id) {",
                  "    const regex = new RegExp(",
                  "        `<input[^>]*id=\"${id}\"[^>]*value=\"([^\"]*)\"`,",
                  "        \"i\"",
                  "    );",
                  "    const match = html.match(regex);",
                  "    return match ? match[1] : null;",
                  "}",
                  "",
                  "const nome = getValueById(\"nome\");",
                  "const email = getValueById(\"email\");",
                  "const endereco = getValueById(\"endereco\");",
                  "const numero = getValueById(\"numero\");",
                  "const bairro = getValueById(\"bairro\");",
                  "const cidade = getValueById(\"cidade\");",
                  "const estado = getValueById(\"estado\");",
                  "const cep = getValueById(\"cep\");",
                  "const site = getValueById(\"site\");",
                  "",
                  "let localPart = null;",
                  "let dominio = null;",
                  "",
                  "const login = site.split(\".\")[1];",
                  "",
                  "pm.environment.set(\"rest_owner_name\", nome);",
                  "pm.environment.set(\"rest_owner_email\", email);",
                  "pm.environment.set(\"rest_owner_login\", login);",
                  "pm.environment.set(\"rest_owner_password\", gerarSenha());",
                  "",
                  "pm.environment.set(\"rest_owner_street\", endereco);",
                  "pm.environment.set(\"rest_owner_number\", numero);",
                  "pm.environment.set(\"rest_owner_neighborhood\", bairro);",
                  "pm.environment.set(\"rest_owner_city\", cidade);",
                  "pm.environment.set(\"rest_owner_state\", estado);",
                  "pm.environment.set(\"rest_owner_zipCode\", cep);",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
